<h1>Artists</h1>

<% @artists.each do |artist| %>
  <%= image_tag(artist.image, :class => 'thumb') %>
<ul>
  <li>Name: <%= link_to(artist.name, artist) %></li>
  <li>Nationality: <%= artist.nationality %></li>
  <li>DOB: <%= artist.dob.strftime('%B %e, %Y') %></li>
  <li>Period: <%= artist.period %></li>
</ul>
<% end %>


